@model CreateEmployeeDto;
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit Employee</h1>

<br />
<br />

<form asp-action="Edit">

    <partial name="EmployeePartailView\_CreateEditPartialView" />

    <div class="my-3">
        <label asp-for="CreateAt" class="col-form-label-lg"></label>
        <input asp-for="CreateAt" class="form-control" />
